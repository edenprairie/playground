{
  "name": "WeRecord",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Wechat recording",
  "homepage_url": "https://github.com/jenac",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Record!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}